{
  "name": "Hr_System",
  "version": "0.3.0",
  "description": "Hr_System project on ES6 RESTful Express API",
  "main": "dist",
  "scripts": {
    "start": "node ./models/index.js && node dist",
    "prestart": "npm run build",
    "live": "node ./models/index.js && nodemon --max_old_space_size=8192 -w src --exec \"babel-node src --presets es2015,stage-0\"",
    "build": "babel src -s -D -d dist --presets es2015,stage-0",
    "test": "newman run postman.json -e env.json",
    "dev": "ENV=dev node ./models/index.js && nodemon --max_old_space_size=8192 -w src --exec \"babel-node src --presets es2015,stage-0\"",
    "db": "babel-node src/db.js --presets es2015",
    "db-migrate": "node ./models/index.js"
  },
  "repository": "developit/express-es6-rest-api",
  "author": "Jason Miller <jason@developit.ca>",
  "license": "MIT",
  "dependencies": {
    "@slack/client": "^3.14.1",
    "async": "^2.6.0",
    "base64-stream": "^0.1.3",
    "body-parser": "^1.13.3",
    "compression": "^1.5.2",
    "cors": "^2.7.1",
    "cron": "^1.2.1",
    "dotenv": "^4.0.0",
    "email-existence": "^0.1.5",
    "eslint": "^3.18.0",
    "express": "^4.13.3",
    "express-jwt": "^5.3.0",
    "express-validator": "^3.2.0",
    "fcm-push": "^1.1.2",
    "google-drive": "0.0.7",
    "googleapis": "^19.0.0",
    "imap": "^0.8.19",
    "in_array": "^1.1.0",
    "mailparser": "^2.0.5",
    "mandrill-api": "^1.0.45",
    "moment": "^2.18.1",
    "mongoose": "^4.9.8",
    "morgan": "^1.8.0",
    "mysql": "^2.13.0",
    "newman": "^3.5.2",
    "node-cron": "^1.1.3",
    "nodemailer": "^4.0.1",
    "nodemailer-express-handlebars": "^2.0.0",
    "nodemailer-smtp-transport": "^2.7.4",
    "password-generator": "^2.1.0",
    "pdfreader": "^0.2.4",
    "pg": "^6.4.2",
    "replace-ext": "^1.0.0",
    "resource-router-middleware": "^0.6.0",
    "sendgrid": "^5.2.0",
    "sequelize": "^3.30.2",
    "sequelize-cli": "^2.8.0",
    "update": "^0.7.4",
    "util": "^0.10.3",
    "uuid": "^3.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.9.0",
    "babel-eslint": "^6.1.2",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "husky": "^0.13.3",
    "nodemon": "^1.14.12"
  }
}